<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="256.0" prefWidth="264.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zsoli.ttstypewriting.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ChoiceBox id="els" fx:id="els" prefWidth="150.0" />
   <ComboBox id="mas" fx:id="mas" prefWidth="150.0" />

    <Label fx:id="welcomeText" />
    <Button id="hello" fx:id="hello" onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
